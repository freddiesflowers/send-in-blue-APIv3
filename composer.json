{
    "name": "freddiesflowers/send-in-blue-apiv3",
    "description": "Freddie's Fork of SendinBlue provided RESTFul API V3 php library",
    "keywords": [
        "sendinblue",
        "swagger",
        "php",
        "sdk",
        "api"
    ],
    "homepage": "https://github.com/freddiesflowers/send-in-blue-APIv3",
    "license": "MIT",
    "authors": [
        {
            "name": "SendinBlue Developers",
            "email": "contact@sendinblue.com",
            "homepage": "https://www.sendinblue.com/"
        },
        {
            "name": "Freddie's Developers",
            "email": "development@freddiesflowers.com",
            "homepage": "https://www.freddiesflowers.com/"
        },
    ],
    "require": {
        "php": ">=5.6",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "autoload": {
        "psr-4": { "SendinBlue\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "SendinBlue\\Client\\" : "test/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.x.x-dev"
        }
    }
}
